<!DOCTYPE html>
<html lang="en">
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%- @getBlock("meta").toHTML() %>
    <title><%= @getPreparedTitle() %></title>
    <%- @getBlock("styles").add(['styles/main.css']).toHTML() %>
    <%- @getBlock("scripts").add(['scripts/bootstrap.min.js', 'scripts/main.js','scripts/scrollspy.js']).toHTML() %>
  </head>
  <body data-spy="scroll" data-target="#navigation" data-offset="0" >

   <%- @content %>

    <%- @partial('nav', {blog:@isBlog()})%>

 

 

    <footer>
      <div class="container">
         <%- @partial('me', false) %>
      </div>
    </footer>

 <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
  </body>
</html>
